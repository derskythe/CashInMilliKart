<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://cashin/CashInService" elementFormDefault="qualified" targetNamespace="http://cashin/CashInService" xmlns:xs="http://www.w3.org/2001/XMLSchema">
