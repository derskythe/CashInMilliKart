          <xs:element minOccurs="0" maxOccurs="1" name="Amounts" type="tns:ArrayOfInt" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="EncashmentResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="EncashmentResult" type="tns:StandardResult" />
      </xs:sequence>
    </xs:complexType>
